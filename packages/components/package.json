{
  "name": "@ds/components",
  "version": "1.0.0-beta",
  "license": "UNLICENSED",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "yarn eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "yarn prettier ./src --write",
    "test": "yarn vitest",
    "check:code": "tsc --noEmit && yarn lint && yarn prettier ./src --check",
    "check:tests": "yarn vitest --run"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.4.5",
    "@open-wc/lit-helpers": "^0.4.0-next.1",
    "lit": "^2.7.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/builder-vite": "^7.0.26",
    "@storybook/web-components": "^7.0.26",
    "@storybook/web-components-vite": "^7.0.26",
    "chromatic": "^6.19.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.26",
    "tsx": "^4.7.1",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.3.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  }
}
